<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta content="Premium Multipurpose Admin & Dashboard Template" name="description" />
    <meta content="Themesbrand" name="author" />
    <title>@ViewData["Title"] - Turbo.az</title>
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/assets/images/favicon.ico" asp-append-version="true">

    <!-- Bootstrap Css -->
    <link href="~/assets/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" asp-append-version="true" />
    <!-- Icons Css -->
    <link href="~/assets/css/icons.min.css" rel="stylesheet" type="text/css" asp-append-version="true" />
    <link rel="shortcut icon" href="~/assets/images/favicon.ico" asp-append-version="true">
    <!-- App Css-->
    <link href="~/assets/css/app.min.css" id="app-style" rel="stylesheet" type="text/css" asp-append-version="true" />
    @await RenderSectionAsync("css", required: false)
</head>
<body data-sidebar="dark" data-layout-mode="light">

    <div id="layout-wrapper">
        <partial name="_SidebarMenu" />
        @RenderBody()
        <partial name="_NavHeader" />
    </div>

    <!-- JAVASCRIPT -->
    <script src="~/assets/libs/jquery/jquery.min.js"></script>
    <script src="~/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/libs/metismenu/metisMenu.min.js"></script>
    <script src="~/assets/libs/simplebar/simplebar.min.js"></script>
    <script src="~/assets/libs/node-waves/waves.min.js"></script>

    <!-- apexcharts -->
    <script src="~/assets/libs/apexcharts/apexcharts.min.js"></script>

    <!-- dashboard init -->
    <script src="~/assets/js/pages/dashboard.init.js"></script>
    <!-- fontawesome icons init -->
    <script src="~/assets/js/pages/fontawesome.init.js"></script>

    <!-- App js -->
    <script src="~/assets/js/app.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
